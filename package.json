{
  "name": "curp",
  "version": "0.0.0",
  "description": "Genera y valida el CURP (Clave Única de Registro de Población) mexicano.",
  "homepage": "",
  "author": {
    "name": "Israel Perales",
    "email": "israel-perales@outlook.com",
    "url": "https://www.ingenieroperales.com"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "keywords": [
    "curp",
    "México",
    "mexicano"
  ],
  "devDependencies": {
    "coveralls": "^3.0.7",
    "eslint": "^6.6.0",
    "prettier": "^1.19.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-config-xo": "^0.27.2",
    "jest": "^24.8.0"
  },
  "engines": {
    "npm": ">= 8.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": ["error", { "singleQuote": true }]
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint . --fix",
    "test": "jest"
  },
  "repository": "ripper2hl/curp",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "GPL-3.0"
}
